<h1>Listing <%= @dojos.count %> Dojos</h1><div >
<table>
	<thead>
	<tr>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
		<th>Actions</th>
	</tr>
	</thead>
	<tbody>
	<% @dojos.each do |dojo| %>
	<tr>
		<td><%= dojo.branch %></td>
		<td><%= dojo.street %></td>
		<td><%= dojo.city %></td>
		<td><%= dojo.state %></td>
		<td>
			<a href="/dojos/<%= dojo.id %>">show</a> | 
			<a href="/dojos/<%= dojo.id %>/edit">edit</a> | 
			<a href="/dojos/<%= dojo.id %>/delete">delete</a>
		</td>
	</tr>
	<% end %>
	</tbody>
</table>
